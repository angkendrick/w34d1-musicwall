<h1 class="h1">View Page</h1>
<div class="col-xs-12 col-sm-6 col-md-8">
  
  <table class="table table-hover table-striped table-bordered">
   
   <% count = 0 %>
    <tr>
      <th>#</th><th>Song</th><th>Artist</th><th>Link</th>
    </tr>
    <% @track.each do |track| %> 
      <tr>
        <td>#<%= count += 1 %></td>
        <td><%= track.song_title %></td>
        <td><%= track.author %></td>
        <td><%= track.url %></td>
      </tr>
    <% end %>
    
  </table>
</div>